# Токен Telegram бота, получаемый от @BotFather
BOT_TOKEN=your_telegram_bot_token_here

# Токен OpenAI API для генерации рекомендаций
OPENAI_API_KEY=your_openai_api_key_here

# Токен ElevenLabs API для генерации голосовых медитаций
ELEVEN_API_KEY=your_elevenlabs_api_key_here

# Опциональные параметры
# Уровень логирования (INFO, DEBUG, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Настройка времени напоминаний по умолчанию (в формате ЧЧ:ММ)
DEFAULT_REMINDER_TIME=20:00 

MONGO_URL=mongodb://<user>:<pass>@localhost:27017